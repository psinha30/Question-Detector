  <style type="text/css">
    .float {
      position: fixed;
      width: 60px;
      height: 60px;
      bottom: 40px;
      right: 40px;
      background-color: green;
      color: white;
      border-radius: 50px;
      text-align: center;
      box-shadow: 2px 2px 3px #999;
    }

    .my-float {
      margin-top: 5px;
    }
  </style>

  <script type="text/javascript">
        setTimeout(function () {
        if(window.location.hash != '#r') {
            window.location.hash = 'r';
            window.location.reload(1);
        }
    }, 500);
  </script>

</head>

<body>
  <a href="#" class="float">
    <i class="fa fa-thumbs-up fa-3x my-float"></i>
  </a>


    <!-- Header -->
  <header id="header">
    <div class="container">

    </div>
  </header>
  <!-- #header -->



  <!-- showing the generated question and tags from ai model   -->


  <!-- form field -->
  <section id="contact">
    <div class="container">
      <div class="row">
        <div class="col-md-13 text-center">
          <h2 class="section-title">The Most colsest question searched according to your asked question is as follows...!</h2>
          <!-- <h5 style="color: red">Please Do Not Spam Us We Are Under Developement</h5> -->
        </div>
      </div>


      <div class="row">
        <div class="col-md-10 text-center">
          <h2 class="section-title"><%= question %></h2>
          <!-- <h5 style="color: red">Please Do Not Spam Us We Are Under Developement</h5> -->
        </div>
      </div>

      <div class="row justify-content-center">

        <div class="col-lg-8 col-md-10">

            <ul class="list-group">
                <%for (var result in results){%>
                  <li class="list-group-item list-group-item-primary"><%=results[result]%></li>
                  
                  <%}%>
            </ul>

        </div>

      </div>
      	<div style="margin-top: 5%" class="col-md-13 text-center">
      			 <h2 class="section-title">Detected tags are as follows...</h2>
      	</div>

      	<div style="margin-top: 5%" class="col-md-13 text-center">
      		<div class="btn-group" role="group" aria-label="Basic example">
                  <%for (var tag in tags){%>
                    <button type="button" class="btn btn-secondary"><%=tags[tag]%></button>
                  <%}%>
    			</div>
    		</div>

    </div>
    <div style="margin-top: 5%" class="col-md-13 text-center">
         <a class="btn btn-full" href="/#contact">Ask Another Question...?</a>
        </div>
  </section>



  <div style="margin-left: 50%, margin-top:5%; ">
            
          </div>

